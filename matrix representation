#include <iostream>
using namespace std;

int main() {
    int n, m;
    cin >> n >> m; // n nodes , m edges
    int adj[n + 1][n + 1];
    for(int i = 0; i <= n; i++) {
        for(int j = 0; j <= n; j++) {
            adj[i][j] = 0;
        }
    }

    // input edges
    for(int i = 0; i < m; i++){
        int u, v;
        cin >> u >> v;

        adj[u][v] = 1;     // edge u â†’ v

        // for undirected graph only
        adj[v][u] = 1;
    }
    cout << "\nAdjacency Matrix:\n";
    for(int i = 1; i <= n; i++) {
        for(int j = 1; j <= n; j++) {
            cout << adj[i][j] << " ";
        }
        cout << "\n";
    }

    return 0;
}
